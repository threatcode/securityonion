{
  "_meta": {
    "documentation": "https://www.elastic.co/guide/en/ecs/current/ecs-base.html",
    "ecs_version": "1.12.2"
  },
  "template": {
    "mappings": {
      "properties": {
        "kismet": {
          "properties": {
            "alerts": {
              "properties": {
                "count": {
                  "type": "long"
                }
              }
            },
            "first_seen": {
              "type": "date"
            },
            "last_seen": {
              "type": "date"
            },
            "seenby": {
              "type": "nested"
            }
          }
        }
      }
    }
  }
}